        "48": "images/rsz_2womanempowerment.png",
        "128": "images/rsz_3womanempowerment.png"
      },
      "icons": {
        "16": "images/rsz_womanempowerment.png",
        "32": "images/rsz_1womanempowerment.png",
        "48": "images/rsz_2womanempowerment.png",
        "128": "images/rsz_3womanempowerment.png"
      }
    },
    "chrome_url_overrides":{
      "newtab": "newtab.html"
    },
    "manifest_version": 2
  }